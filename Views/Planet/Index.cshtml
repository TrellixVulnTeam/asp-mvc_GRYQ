@inject redux.Services.PlanetService  planetService

@{
    Layout = "_Layout";
}
<h1>Cac hanh tinh trong he mat troi<h1>

<div class="list-group">
    @foreach (var planet in planetService)
{
    <a class="list-group-item list-group-item-action" asp-action="@planet.VnName" asp-controller="Planet">
        @planet.Name - @planet.VnName
    </a>
}
</div>